{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NPM_CONFIG_CACHE": "/tmp/.npm"
  },
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  }
}
